{
    "compilerOptions": {
      "target": "es5",
      "lib": ["dom", "es2015"],
      "allowJs": true,
      "skipLibCheck": true,
      "esModuleInterop": true,
      "allowSyntheticDefaultImports": true,
      "strict": true,
      "forceConsistentCasingInFileNames": true,
      "moduleResolution": "node",
      "module": "esnext",
      "jsx": "react-jsx"
    },
    "include": [
      "src"
    ]
  }